<?xml version="1.0" encoding="UTF-8" ?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" 
    xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    
  <modelVersion>4.0.0</modelVersion>
	<parent>
    	<groupId>com.cn.51park</groupId>
    	<artifactId>51park-einvoice</artifactId>
    	<version>0.0.1-SNAPSHOT</version>
  	</parent>
	<artifactId>51park-einvoice-web</artifactId>
  	<packaging>war</packaging>
  	<name>web</name>
  	
  	<build>
    	<finalName>Einvoice</finalName>
    	<plugins>  
			<plugin>  
				<groupId>org.apache.maven.plugins</groupId>  
                <artifactId>maven-compiler-plugin</artifactId>  
                <version>3.0</version>  
                <configuration>   
                	<source>1.8</source>  
                    <target>1.8</target>  
                    <encoding>UTF-8</encoding>  
                    <verbal>true</verbal>  
                </configuration>  
            </plugin>   
        </plugins>  
        <resources>
			<resource>
				<directory>src/main/resources</directory>
				<excludes>
					<exclude>conf-dev/**</exclude>
					<exclude>conf-test/**</exclude>
					<exclude>conf-prod/**</exclude>
					<exclude>conf-beta/**</exclude>
				</excludes>
			</resource>
			<resource>
				<directory>src/main/resources/conf-${env}</directory>
			</resource>
			<resource>
				<filtering>true</filtering>
				<directory>src/main/resources</directory>
				<includes>
					<include>conf-dev</include>
					<include>conf-test</include>
					<include>conf-prod</include>
					<include>conf-beta</include>
				</includes>
			</resource>
		</resources>
  	</build>
	<dependencies>
		<dependency>
			<groupId>com.cn.51park</groupId>
			<artifactId>51park-einvoice-common</artifactId>
			<version>${project.version}</version>
		</dependency>
