			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>com.cn.51park</groupId>
			<artifactId>51park-einvoice-controller</artifactId>
			<version>${project.version}</version>
		</dependency>
    	<dependency>
			<groupId>org.springframework</groupId>
